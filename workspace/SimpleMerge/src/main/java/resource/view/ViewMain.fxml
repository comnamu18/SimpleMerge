<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="root" prefHeight="600.0" prefWidth="800.0" styleClass="background" stylesheets="@theme.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainController">
   <left>
      <BorderPane fx:id="leftBorder" BorderPane.alignment="CENTER">
         <top>
            <AnchorPane fx:id="leftButtonPane" prefHeight="0.0" prefWidth="440.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="leftLoad" mnemonicParsing="false" onAction="#LeftLoadClick" styleClass="blackbutton" stylesheets="@theme.css" text="Load" />
                  <Button fx:id="leftEdit" layoutX="51.0" mnemonicParsing="false" onAction="#LeftEditClick" styleClass="blackbutton" stylesheets="@theme.css" text="Edit" />
                  <Button fx:id="leftSave" layoutX="97.0" mnemonicParsing="false" onAction="#LeftSaveClick" styleClass="blackbutton" stylesheets="@theme.css" text="Save" />
               </children>
            </AnchorPane>
         </top>
         <center>
            <BorderPane fx:id="leftTextPane" BorderPane.alignment="CENTER">
               <center>
                  <TextArea fx:id="leftText" editable="false" prefHeight="494.0" prefWidth="440.0" BorderPane.alignment="CENTER" />
               </center>
               <top>
                  <TextField fx:id="leftTitle" editable="false" BorderPane.alignment="CENTER" />
               </top>
            </BorderPane>
         </center>
      </BorderPane>
   </left>
   <right>
      <BorderPane fx:id="rightBorder" BorderPane.alignment="CENTER">
         <top>
            <AnchorPane fx:id="rightButtonPane" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="rightLoad" mnemonicParsing="false" onAction="#RightLoadClick" styleClass="blackbutton" stylesheets="@theme.css" text="Load" />
                  <Button fx:id="rightEdit" layoutX="51.0" mnemonicParsing="false" onAction="#RightEditClick" styleClass="blackbutton" stylesheets="@theme.css" text="Edit" />
                  <Button fx:id="rightSave" layoutX="97.0" mnemonicParsing="false" onAction="#RightSaveClick" styleClass="blackbutton" stylesheets="@theme.css" text="Save" />
               </children>
            </AnchorPane>
         </top>
         <center>
            <BorderPane fx:id="rightTextPane" BorderPane.alignment="CENTER">
               <center>
                  <TextArea fx:id="rightText" editable="false" BorderPane.alignment="CENTER" />
               </center>
               <top>
                  <TextField fx:id="rightTitle" editable="false" BorderPane.alignment="CENTER" />
               </top>
            </BorderPane>
         </center>
      </BorderPane>
   </right>
   <center>
      <AnchorPane fx:id="CenterButtonPane" maxWidth="218.0" prefHeight="600.0" prefWidth="117.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="Compare" layoutY="349.0" mnemonicParsing="false" onAction="#CompareClick" styleClass="blackbutton" stylesheets="@theme.css" text="Compare" />
            <Button fx:id="CopyToLeft" layoutX="13.0" layoutY="314.0" mnemonicParsing="false" onAction="#CopyToLeftClick" styleClass="blackbutton" stylesheets="@theme.css" text="&lt;--" />
            <Button fx:id="CopyToRight" layoutX="13.0" layoutY="385.0" mnemonicParsing="false" onAction="#CopyToRightClick" styleClass="blackbutton" stylesheets="@theme.css" text="--&gt;" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
